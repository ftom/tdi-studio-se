<!-- $Id: tFileZip_java.xml 1528 2007-01-19 17:03:36 +0000 (星期五, 19 一月 2007) smallet $ -->
<COMPONENT>
   <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"
	COMPATIBILITY="ALL"
    AUTHOR="Talend"
 	RELEASE_DATE="20050320A"
    STARTABLE="true"
	PARTITIONING="AUTO"
  LOG4J_ENABLED="true"
   >
   <SIGNATURE></SIGNATURE>
  </HEADER>

  <FAMILIES>
    <FAMILY>File/Management</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER NAME="SOURCE" FIELD="DIRECTORY" REQUIRED="true" NUM_ROW="1">
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="SUB_DIRECTROY" FIELD="CHECK" REQUIRED="true" NUM_ROW="1">
      <DEFAULT>true</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="TARGET" FIELD="FILE" REQUIRED="true" NUM_ROW="2">
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/archive.zip"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="MKDIR" FIELD="CHECK"  NUM_ROW="3">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="LEVEL" FIELD="CLOSED_LIST" REQUIRED="true" NUM_ROW="4">
      <ITEMS DEFAULT="4">
        <ITEM NAME="BEST" VALUE="9" />
        <ITEM NAME="NORMAL" VALUE="4" />
        <ITEM NAME="FAST" VALUE="0" />
      </ITEMS>
    </PARAMETER>

    <PARAMETER NAME="ALL_FILES" FIELD="CHECK" REQUIRED="true" NUM_ROW="5">
      <DEFAULT>true</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="MASK" FIELD="TABLE" REQUIRED="false" NUM_ROW="6" SHOW_IF="ALL_FILES == 'false'">
      <ITEMS>
     	<ITEM NAME="FILEMASK" />
      </ITEMS>
    </PARAMETER>

    <PARAMETER NAME="ENCODING" FIELD="ENCODING_TYPE" NUM_ROW="7" REQUIRED="true" REPOSITORY_VALUE="ENCODING">
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="OVERWRITE" FIELD="CHECK" REQUIRED="true" NUM_ROW="8">
      <DEFAULT>true</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="ENCRYPT_FILES" FIELD="CHECK" REQUIRED="false" NUM_ROW="9">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="ENCRYPT_METHOD" FIELD="CLOSED_LIST" REQUIRED="false" NUM_ROW="10" SHOW_IF="ENCRYPT_FILES == 'true'">
      <ITEMS DEFAULT="DEFAULT">
          <ITEM NAME="DEFAULT" VALUE="DEFAULT" />
          <ITEM NAME="AES256" VALUE="AES256"/>
      </ITEMS>
    </PARAMETER>

     <PARAMETER NAME="PASSWORD" FIELD="PASSWORD" REQUIRED="false" NUM_ROW="11" SHOW_IF="ENCRYPT_FILES == 'true'">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="ZIP64_MODE" FIELD="CLOSED_LIST" NUM_ROW="12" NOT_SHOW_IF="(ENCRYPT_FILES == 'true') AND (ENCRYPT_METHOD == 'AES256')">
      <ITEMS DEFAULT="ASNEEDED">
	      <ITEM NAME="ASNEEDED" VALUE="ASNEEDED" />
	      <ITEM NAME="ALWAYS" VALUE="ALWAYS"/>
	      <ITEM NAME="NEVER" VALUE="NEVER"/>
      </ITEMS>
    </PARAMETER>

  </PARAMETERS>

   <CODEGENERATION>
     <IMPORTS>
       <IMPORT NAME="COMMONS-COMPRESS-1.6" MODULE="commons-compress-1.6.jar" UrlPath="platform:/plugin/org.talend.libraries.apache.common/lib/commons-compress-1.6.jar" REQUIRED="true" />
       <IMPORT NAME="Encrypt-Zip" MODULE="checkArchive.jar" REQUIRED="true" />
       <IMPORT NAME="zip4j_1.3.1.jar" MODULE="zip4j_1.3.1.jar" REQUIRED="true" />
       <IMPORT NAME="talendzip.jar" MODULE="talendzip.jar" REQUIRED="true" />
     </IMPORTS>
   </CODEGENERATION>

  <RETURNS>
  		<RETURN NAME="ARCHIVE_FILEPATH" TYPE="id_String" AVAILABILITY="AFTER" />
		<RETURN NAME="ARCHIVE_FILENAME" TYPE="id_String" AVAILABILITY="AFTER" />
  </RETURNS>

</COMPONENT>
