		</PARAMETER>
			
		<PARAMETER NAME="AUTO_COMMIT" FIELD="CHECK" NUM_ROW="10" SHOW="false">  
			<DEFAULT>false</DEFAULT>    
    	</PARAMETER>
    	
		<PARAMETER
	      NAME="ENCODING"
	      FIELD="ENCODING_TYPE"
	      NUM_ROW="200"
	      REQUIRED="true"
