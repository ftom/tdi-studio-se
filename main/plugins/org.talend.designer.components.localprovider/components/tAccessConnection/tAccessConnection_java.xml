    />
    
	<PARAMETER NAME="DB_VERSION" FIELD="CLOSED_LIST" NUM_ROW="10"
			REPOSITORY_VALUE="DB_VERSION">
			<ITEMS DEFAULT="Access_2003">
				<ITEM NAME="Access_2003" VALUE="Access_2003"/>
				<ITEM NAME="Access_2007" VALUE="Access_2007" />
			</ITEMS>
		</PARAMETER>			
			
