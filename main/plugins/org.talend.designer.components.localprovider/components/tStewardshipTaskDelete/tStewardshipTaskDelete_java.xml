<?xml version="1.0" encoding="UTF-8"?>

<COMPONENT> 
  <HEADER AUTHOR="Talend" COMPATIBILITY="ALL" 
  		DATA_AUTO_PROPAGATE="false" PLATEFORM="ALL" 
  		RELEASE_DATE="20120820A" SERIAL=""  LOG4J_ENABLED="true"
  		STARTABLE="true" STATUS="ALPHA" VERSION="0.102"> 
    <SIGNATURE/> 
  </HEADER>  
  <FAMILIES> 
    <FAMILY>Talend MDM</FAMILY>
	<FAMILY>Data_Quality</FAMILY>
  </FAMILIES>  
  <DOCUMENTATION> 
    <URL/> 
  </DOCUMENTATION>  
  <CONNECTORS> 
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="0" /> 
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
	<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
	<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
	<CONNECTOR CTYPE="COMPONENT_OK" />
	<CONNECTOR CTYPE="COMPONENT_ERROR" />
	<CONNECTOR CTYPE="RUN_IF" />
  </CONNECTORS>  
  <PARAMETERS> 
	<PARAMETER NAME="HOST" FIELD="TEXT" NUM_ROW="2" REQUIRED="true" GROUP="CONNECTION_GRP">
    	<DEFAULT>"localhost"</DEFAULT>
    </PARAMETER>  
    <PARAMETER NAME="PORT" FIELD="TEXT" NUM_ROW="2" REQUIRED="true" GROUP="CONNECTION_GRP">
    	<DEFAULT>8180</DEFAULT>
    </PARAMETER>  
   	
    <PARAMETER NAME="USERNAME" FIELD="TEXT" NUM_ROW="5" GROUP="CONNECTION_GRP">
    	<DEFAULT>"username"</DEFAULT>
    </PARAMETER>  
    <PARAMETER NAME="PASSWORD" FIELD="PASSWORD" NUM_ROW="5" GROUP="CONNECTION_GRP">
    	<DEFAULT>"password"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="DELETE_BY_ID" FIELD="CHECK" REQUIRED="true" NUM_ROW="7" GROUP="TASK_OPTION">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	
	<PARAMETER NAME="SPECIFY_TASK_ID" FIELD="CHECK" REQUIRED="true" NUM_ROW="8" SHOW_IF="(DELETE_BY_ID=='true')" GROUP="TASK_OPTION">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	
	<PARAMETER NAME="TASK_ID" FIELD="TEXT" REQUIRED="true" NUM_ROW="8" SHOW_IF="isShow[SPECIFY_TASK_ID] AND (SPECIFY_TASK_ID=='true')" GROUP="TASK_OPTION">
    	<DEFAULT>""</DEFAULT>
    </PARAMETER>
	
	<PARAMETER NAME="ID_FIELD" FIELD="PREV_COLUMN_LIST" REQUIRED="true" NUM_ROW="9" SHOW_IF="isShow[SPECIFY_TASK_ID] AND (SPECIFY_TASK_ID=='false')" GROUP="TASK_OPTION" />
    
    <PARAMETER NAME="TASKTYPE" FIELD="CLOSED_LIST" NUM_ROW="10" SHOW_IF="DELETE_BY_ID == 'false'"  GROUP="TASK_OPTION">
	    <ITEMS DEFAULT="RESOLUTION">
			<ITEM NAME="RESOLUTION" VALUE="1" />
			<ITEM NAME="DATA" VALUE="2" />
		</ITEMS>  
	</PARAMETER>
	<PARAMETER NAME="OWNER" FIELD="TEXT" NUM_ROW="13" SHOW_IF="DELETE_BY_ID == 'false'"  GROUP="TASK_OPTION">
    	<DEFAULT>"owner"</DEFAULT>
    </PARAMETER>  
    <PARAMETER NAME="TAG" FIELD="TEXT" NUM_ROW="16" SHOW_IF="DELETE_BY_ID == 'false'"  GROUP="TASK_OPTION">
    	<DEFAULT>"tag"</DEFAULT>
    </PARAMETER> 
    <PARAMETER NAME="START_DATETIME" FIELD="TEXT" NUM_ROW="20" SHOW_IF="DELETE_BY_ID == 'false'"  GROUP="TASK_OPTION">
    	 <DEFAULT>"yyyy-MM-dd HH:mm:ss"</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="END_DATETIME" FIELD="TEXT" NUM_ROW="20" SHOW_IF="DELETE_BY_ID == 'false'"  GROUP="TASK_OPTION">
		 <DEFAULT>"yyyy-MM-dd HH:mm:ss"</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="STATUS" FIELD="CLOSED_LIST" NUM_ROW="30" SHOW_IF="DELETE_BY_ID == 'false'"  GROUP="TASK_OPTION">
		 <ITEMS DEFAULT="All">
	    	<ITEM NAME="All" VALUE="null" />
			<ITEM NAME="NEW" VALUE="new" />
			<ITEM NAME="RESOLVED" VALUE="resolved" />
			<ITEM NAME="LOCKED" VALUE="locked" />
			<ITEM NAME="PENDING" VALUE="pending" />
		 </ITEMS> 
    </PARAMETER>
    <PARAMETER NAME="STAR_RANKING" FIELD="CLOSED_LIST" NUM_ROW="35" SHOW_IF="DELETE_BY_ID == 'false'"  GROUP="TASK_OPTION">
	    <ITEMS DEFAULT="All">
	    	<ITEM NAME="All" VALUE="null" />
			<ITEM NAME="0" VALUE="0" />
			<ITEM NAME="1" VALUE="1" />
			<ITEM NAME="2" VALUE="2" />
			<ITEM NAME="3" VALUE="3" />
			<ITEM NAME="4" VALUE="4" />
			<ITEM NAME="5" VALUE="5" />
		</ITEMS>  
	</PARAMETER>
    <PARAMETER NAME="LIMIT" FIELD="TEXT" NUM_ROW="40" REQUIRED="true" SHOW_IF="DELETE_BY_ID == 'false'">
    	<DEFAULT>50</DEFAULT>
    </PARAMETER> 
  </PARAMETERS>  
  
  <ADVANCED_PARAMETERS> 
     
  </ADVANCED_PARAMETERS>  
  <CODEGENERATION>
  	<IMPORTS>
  		<IMPORT NAME="talend-stewardship-task" MODULE="talend-stewardshipTask.jar" REQUIRED="true" />
  	</IMPORTS>
  </CODEGENERATION>  
  <RETURNS> 
   <RETURN AVAILABILITY="AFTER" NAME="DELETED_TASK_NUMBER" TYPE="id_Integer"/> 
  </RETURNS> 
</COMPONENT>
