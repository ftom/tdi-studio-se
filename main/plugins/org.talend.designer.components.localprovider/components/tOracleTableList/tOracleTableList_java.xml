<COMPONENT>
	<HEADER
	    PLATEFORM="ALL"
	    SERIAL=""
	    VERSION="0.101"
	    STATUS="ALPHA"
	    COMPATIBILITY="ALL"
	    AUTHOR="Talend"
	    RELEASE_DATE="20050320A"
	    STARTABLE="true"
	    LOG4J_ENABLED="true"
	>
    	<SIGNATURE />
	</HEADER>

  <FAMILIES>
    <FAMILY>Databases/Oracle</FAMILY>
  </FAMILIES>

	<DOCUMENTATION>
		<URL/>
	</DOCUMENTATION>

  	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_OUTPUT="0" MAX_INPUT="0" />
		<CONNECTOR CTYPE="ITERATE" MAX_INPUT="1" />
	    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
	    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
	    <CONNECTOR CTYPE="COMPONENT_OK"/>
	    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
	    <CONNECTOR CTYPE="RUN_IF"/>
  	</CONNECTORS>

	<PARAMETERS>
		
		<!-- Oracle -->
		<PARAMETER 
			NAME="CONNECTION" 
			FIELD="COMPONENT_LIST"
			REQUIRED="true" FILTER="tOracleConnection" 
			NUM_ROW="5"
			DYNAMIC_SETTINGS="true"
		/>
		
		<PARAMETER NAME="WHERE_CLAUSE" FIELD="TEXT" NUM_ROW="10">
			<DEFAULT>"TNAME not like '%$%'"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="NOTE" FIELD="LABEL" NUM_ROW="15">
			<DEFAULT>*Note: Table names are always in upper case. Example for the where clause: "TNAME not like '%$%' AND (TNAME like 'S%' OR TNAME='TALEND')"</DEFAULT>
		</PARAMETER>
		
<!-- 
		<PARAMETER 
		  NAME="DATABASE_NAME" 
		  FIELD="TEXT" 
		  NUM_ROW="5"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		-->

	</PARAMETERS>
  
	<ADVANCED_PARAMETERS/>	

  	<CODEGENERATION/>

 	<RETURNS>
 		<RETURN NAME="CURRENT_TABLE" TYPE="id_String" AVAILABILITY="FLOW" />
		<RETURN NAME="NB_TABLE" TYPE="id_Integer" AVAILABILITY="AFTER" />
 	</RETURNS>
</COMPONENT>
